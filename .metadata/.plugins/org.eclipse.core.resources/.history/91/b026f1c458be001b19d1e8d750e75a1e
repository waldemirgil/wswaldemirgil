package repeticao;

public class ExemploFor {

	public static void main(String[] args) {
		
		
		/*
		 * Sintaxe do for:
		 * 1º variavel que vai contar
		 * 2º condição
		 * 3º como ele vai contar?
		 */
		
		for (int contador=0;contador<500;contador+=1) {
			System.out.println(contador);
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
}
